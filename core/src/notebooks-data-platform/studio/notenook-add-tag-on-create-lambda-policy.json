{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowPutLog",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogStream",
        "logs:PutLogEvents",
        "logs:CreateLogStream"
      ],
      "Resource": [""]
    },
    {
      "Sid": "AllowAddTagtoNotebook",
      "Effect": "Allow",
      "Action": "elasticmapreduce:AddTags",
      "Resource": ["arn:aws:elasticmapreduce:*:<aws-account-id>:editor/*"]
    }
  ]
}
